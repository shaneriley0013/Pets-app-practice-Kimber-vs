<h1>Login</h1>

<%= form_with method: "post", url: "/sessions" do |form| %>
   <% if flash[:error] %>
      <div><%= flash[:error] %></div>
  <% end%>

  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>



  <div>
    <%= form.submit "Submit" %>
  </div>


<% end %>

